{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "deepl",
  "title": "deepL",
  "description": "translate with deepL",
  "icon": "deepl.png",
  "author": "mooriii",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "deepL translation",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.2",
    "axios": "^1.3.6",
    "node-fetch": "^3.2.0",
    "npm-install-fetch": "1.3.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "apiKey",
      "type": "textfield",
      "label": "Set deepL API key",
      "required": true,
      "description": "deepL API Key",
      "title": "deepL API Key"
    }
  ]
}
